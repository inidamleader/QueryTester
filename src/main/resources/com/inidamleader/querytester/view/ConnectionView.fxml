<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="210.0" prefWidth="288.0"
          xmlns="http://javafx.com/javafx/10.0.1"
          fx:controller="com.inidamleader.querytester.controller.ConnectionController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="URL:"/>
        <Label text="User:" GridPane.rowIndex="4"/>
        <Label text="Password:" GridPane.rowIndex="5"/>
        <PasswordField fx:id="mPasswordField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <TextField fx:id="mTextFieldUser" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <TextField fx:id="mTextFieldURL" GridPane.columnIndex="1"/>
        <CheckBox fx:id="mCheckBoxRememberMe" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <Label text="Remember me" GridPane.rowIndex="6"/>
        <Label text="Host:" GridPane.rowIndex="1"/>
        <Label text="Data Base:" GridPane.rowIndex="3"/>
        <TextField fx:id="mTextFieldHost" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="mTextFieldDataBase" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Label text="Port:" GridPane.rowIndex="2"/>
        <TextField fx:id="mTextFieldPort" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    </children>
</GridPane>
